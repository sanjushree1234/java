
public class Main
{
	public static void main(String[] args) {
    int a[] = {1,2,3,4,6}, sum=0;
    int expected_number = a.length + 1;
    int total_sum = expected_number*(expected_number+1)/2;
    for ( int i=0; i<a.length;i++){
        sum = sum + a[i];
    }
    System.out.print("the missing number " + (total_sum - sum));
	}
}
